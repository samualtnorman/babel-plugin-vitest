		"@samual/rollup-config": "^0.0.0",
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/babel__core": "^7.20.5",
		"@types/node": "^20.11.30",
		"rollup": "^4.13.2",
		"typescript": "^5.4.3"
	},
	"engines": {
		"node": "^18 || >=20",
		"pnpm": "^10"
	},
	"private": true
}
